<?xml version="1.0" encoding='UTF-8'?>
<odoo>
	<data noupdate="1">
 
		<record model="ir.cron" id="ariis_mail_tratar_lecturas_mail">
			<field name="name">Ariis: Lecturas MIR (Tratar)</field>
			<field name="model_id" ref="ariis_mir.model_ariis_mir_lectura"/>
			<field name="state">code</field>
			<field name="code">model.tratarlecturas()</field>
			<field name="user_id" ref="base.user_root" />
			<field name="interval_number">5</field>
			<field name="interval_type">minutes</field>
			<field name="numbercall">-1</field>
			<field eval="False" name="doall" />
			<field name="priority">15</field>
		</record>
		
		<record model="ir.cron" id="ariis_mail_borrar_lecturas_mail">
			<field name="name">Ariis: Lecturas MIR (Borrar)</field>
			<field name="model_id" ref="ariis_mir.model_ariis_mir_lectura"/>
			<field name="state">code</field>
			<field name="code">model.removeOlds(-31)</field>
			<field name="user_id" ref="base.user_root" />
			<field name="interval_number">1</field>
			<field name="interval_type">days</field>
			<field name="numbercall">-1</field>
			<field eval="False" name="doall" />
			<field name="priority">15</field>
		</record>
	</data>
</odoo>